{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {},
    "format": {
      "cache": false
    },
    "typecheck": {},
    "test": {
      "dependsOn": ["^build"]
    },
    "e2e": {
      "cache": false
    }
  }
}
